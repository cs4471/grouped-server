{
  <% unless defined? @error %>
    "messages": [
    	<% @messages.each_with_index do |message, index| %>
    	{
    		"id": <%= message.id %>,
    		"member_id": <%= message.member_id %>,
    		"message": "<%= message.message %>"
    	}

    		<% unless index == @messages.length - 1 %>
    			,
    		<% end %>
    	<% end %>
    ]

  <% else %>
    "error": <%= @error %>
  <% end %>
}